{
	"name": "@u27n/webpack",
	"version": "0.2.0",
	"main": "./dist/cjs/index.js",
	"module": "./dist/es/index.js",
	"exports": {
		".": {
			"module": "./dist/es/index.js",
			"import": "./dist/es/index.js",
			"default": "./dist/cjs/index.js"
		},
		"./runtime": {
			"module": "./dist/es/runtime/index.js",
			"import": "./dist/es/runtime/index.js",
			"default": "./dist/cjs/runtime/index.js"
		}
	},
	"scripts": {
		"clean": "rimraf ./dist/**/*",
		"build": "npm run clean && concurrently npm:build:*",
		"build:es": "tsc -p tsconfig-es.json",
		"build:cjs": "tsc -p tsconfig-cjs.json",
		"start": "tsc -p tsconfig-cjs.json -w",
		"lint": "eslint --no-error-on-unmatched-pattern --config .eslintrc.json ./src/**/*",
		"prepack": "npm run build && npm run lint"
	},
	"devDependencies": {
		"@mpt/eslint-rules": "^3.0.0",
		"@u27n/core": "^1.14.1",
		"@u27n/typescript": "^1.1.1",
		"concurrently": "^7.4.0",
		"html-webpack-plugin": "^5.5.0",
		"preact": "^10.11.0",
		"rimraf": "^3.0.2",
		"ts-loader": "^9.4.1",
		"typescript": "^4.8.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.1"
	},
	"peerDependencies": {
		"webpack": "^5.74.0"
	},
	"dependencies": {
		"estree-walker": "^2.0.2"
	}
}
