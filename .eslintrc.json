{
	"root": true,
	"extends": "./node_modules/@mpt/eslint-rules/typescript.json",
	"parserOptions": {
		"tsconfigRootDir": ".",
		"project": "./tsconfig.json"
	},
	"rules": {
		"@typescript-eslint/explicit-member-accessibility": "off",
		"camelcase": [
			"error",
			{
				"properties": "always",
				"ignoreImports": true,
				"ignoreGlobals": true,
				"allow": [
					"^_u27nw_"
				]
			}
		]
	}
}
